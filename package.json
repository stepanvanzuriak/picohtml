{
  "name": "picohtml",
  "version": "0.0.3",
  "description": "HTML template strings",
  "main": "./dist/picohtml.js",
  "repository": "https://github.com/stepanvanzuriak/picohtml",
  "author": "stepanvanzuriak <stepanvanzuriak@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "http-server": "^0.11.1",
    "husky": "^1.0.0-rc.15",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "parcel": "^1.9.7",
    "puppeteer": "^1.8.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "typings": "picohtml.d.ts",
  "keywords": [
    "pico",
    "html",
    "dom",
    "render",
    "template",
    "template-string"
  ],
  "scripts": {
    "dev": "parcel watch src/index.ts --out-file picohtml.js",
    "build": "parcel build src/index.ts --out-file picohtml.js --experimental-scope-hoisting",
    "clean": "rimraf dist",
    "clean:test": "rimraf test/picohtml.js",
    "build:test": "parcel build src/index.ts --out-dir test --out-file picohtml.js --experimental-scope-hoisting",
    "test": "npm run clean:test && npm run build:test && jest",
    "prepublish": "npm run test && npm run clean && npm run build"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  }
}
